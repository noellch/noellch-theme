    "list.inactiveSelectionBackground": "#252423",
    "list.inactiveSelectionForeground": "#c9bda8",
    "list.hoverBackground": "#262524",
    "list.hoverForeground": "#c9bda8",
    "list.highlightForeground": "#d3b67c",
    "list.focusBackground": "#302f2e",
    "list.focusForeground": "#d8ccb8",
    "statusBar.background": "#181716",
    "statusBar.foreground": "#8e8478",
    "statusBar.border": "#222120",
    "statusBar.debuggingBackground": "#5c3a3a",
    "statusBar.debuggingForeground": "#d8ccb8",
    "statusBar.noFolderBackground": "#1d1c1a",
    "statusBarItem.activeBackground": "#353433",
    "statusBarItem.hoverBackground": "#2a2928",
    "statusBarItem.prominentBackground": "#2a2928",
    "statusBarItem.remoteBackground": "#605c58",
    "statusBarItem.remoteForeground": "#f0e8d8",
    "tab.activeBackground": "#1d1c1a",
    "tab.activeForeground": "#d8ccb8",
    "tab.inactiveBackground": "#181716",
    "tab.inactiveForeground": "#7a7876",
    "tab.border": "#181716",
    "tab.activeBorderTop": "#acaa66",
    "tab.unfocusedActiveBorderTop": "#6b5a4a",
    "editorGroupHeader.tabsBackground": "#181716",
    "editorGroupHeader.tabsBorder": "#222120",
    "terminal.background": "#1d1c1a",
    "terminal.foreground": "#c9bda8",
    "terminal.ansiBlack": "#2a2928",
    "terminal.ansiRed": "#c4828f",
    "terminal.ansiGreen": "#649064",
    "terminal.ansiYellow": "#acaa66",
    "terminal.ansiBlue": "#8098b1",
    "terminal.ansiMagenta": "#a27fa0",
    "terminal.ansiCyan": "#6db0aa",
    "terminal.ansiWhite": "#c9bda8",
    "terminal.ansiBrightBlack": "#797167",
    "terminal.ansiBrightRed": "#cf8e98",
    "terminal.ansiBrightGreen": "#82a882",
    "terminal.ansiBrightYellow": "#bbb87a",
    "terminal.ansiBrightBlue": "#94acc2",
    "terminal.ansiBrightMagenta": "#b494b2",
    "terminal.ansiBrightCyan": "#82bcb6",
    "terminal.ansiBrightWhite": "#e0d6c4",
    "terminalCursor.foreground": "#d3b67c",
    "scrollbar.shadow": "#0f0e0d",
    "scrollbarSlider.activeBackground": "#58565466",
    "scrollbarSlider.background": "#38373644",
    "scrollbarSlider.hoverBackground": "#58565455",
    "input.background": "#222120",
    "input.foreground": "#c9bda8",
    "input.border": "#302f2e",
    "input.placeholderForeground": "#656362",
    "inputOption.activeBorder": "#acaa66",
    "inputOption.activeBackground": "#acaa6633",
    "dropdown.background": "#222120",
    "dropdown.border": "#302f2e",
    "dropdown.foreground": "#c9bda8",
    "button.background": "#605c58",
    "button.foreground": "#f0e8d8",
    "button.hoverBackground": "#726e6a",
    "button.secondaryBackground": "#353433",
    "button.secondaryForeground": "#c9bda8",
    "button.secondaryHoverBackground": "#444342",
    "badge.background": "#b6766c",
    "badge.foreground": "#f0e8d8",
    "panel.background": "#1a1918",
    "panel.border": "#2a2928",
    "panelTitle.activeBorder": "#acaa66",
    "panelTitle.activeForeground": "#d8ccb8",
    "panelTitle.inactiveForeground": "#7a7876",
    "peekView.border": "#605c58",
    "peekViewEditor.background": "#1d1c1a",
    "peekViewEditor.matchHighlightBackground": "#ba896d30",
    "peekViewResult.background": "#1a1918",
    "peekViewResult.fileForeground": "#c9bda8",
    "peekViewResult.lineForeground": "#9a9590",
    "peekViewResult.matchHighlightBackground": "#ba896d30",
    "peekViewResult.selectionBackground": "#302f2e",
    "peekViewResult.selectionForeground": "#d8ccb8",
    "peekViewTitle.background": "#222120",
    "peekViewTitleDescription.foreground": "#8e8478",
    "peekViewTitleLabel.foreground": "#d8ccb8",
    "minimap.findMatchHighlight": "#ba896d44",
    "minimap.selectionHighlight": "#44434244",
    "minimapGutter.addedBackground": "#649064",
    "minimapGutter.modifiedBackground": "#acaa66",
    "minimapGutter.deletedBackground": "#c4828f",
    "breadcrumb.foreground": "#7a7876",
    "breadcrumb.focusForeground": "#c9bda8",
    "breadcrumb.activeSelectionForeground": "#d8ccb8",
    "breadcrumbPicker.background": "#222120",
    "gitDecoration.addedResourceForeground": "#649064",
    "gitDecoration.modifiedResourceForeground": "#acaa66",
    "gitDecoration.deletedResourceForeground": "#c4828f",
    "gitDecoration.untrackedResourceForeground": "#6db0aa",
    "gitDecoration.ignoredResourceForeground": "#454443",
    "gitDecoration.conflictingResourceForeground": "#ba896d",
    "notificationCenter.border": "#302f2e",
    "notificationCenterHeader.background": "#222120",
    "notificationCenterHeader.foreground": "#c9bda8",
    "notifications.background": "#222120",
    "notifications.border": "#302f2e",
    "notifications.foreground": "#c9bda8",
    "quickInput.background": "#222120",
    "quickInput.foreground": "#c9bda8",
    "quickInputList.focusBackground": "#302f2e",
    "quickInputList.focusForeground": "#d8ccb8",
    "quickInputTitle.background": "#222120",
    "diffEditor.insertedTextBackground": "#64906418",
    "diffEditor.removedTextBackground": "#c4828f18"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#797167",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#649064"
      }
    },
    {
      "name": "String escape / Regex",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#6db0aa"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#ba896d"
      }
    },
    {
      "name": "Boolean / Null / Undefined",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#b6766c"
      }
    },
    {
      "name": "Constant (other)",
      "scope": [
        "constant.other",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#ba896d"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.new",
        "keyword.operator.delete",
        "keyword.operator.typeof",
        "keyword.operator.instanceof"
      ],
      "settings": {
        "foreground": "#b6766c"
      }
    },
    {
      "name": "Keyword operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#8b939c"
      }
    },
    {
      "name": "Storage (var, let, const, func, class)",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#b6766c"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "meta.function-call entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#d3b67c"
      }
    },
    {
      "name": "Function call",
      "scope": [
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#d3b67c"
      }
    },
    {
      "name": "Method",
      "scope": [
        "meta.method.declaration entity.name.function",
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#d3b67c"
      }
    },
    {
      "name": "Class / Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#acaa66"
      }
    },
    {
      "name": "Interface",
      "scope": [
        "entity.name.type.interface"
      ],
      "settings": {
        "foreground": "#acaa66",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type parameter (generics)",
      "scope": [
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#6db0aa"
      }
    },
    {
      "name": "Enum member",
      "scope": [
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#8098b1"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#c9bda8"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter",
        "meta.parameters variable"
      ],
      "settings": {
        "foreground": "#a9a292",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#8098b1"
      }
    },
    {
      "name": "this / self",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.special.self"
      ],
      "settings": {
        "foreground": "#b6766c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Import / Export / From",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#b6766c"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments"
      ],
      "settings": {
        "foreground": "#8b847d"
      }
    },
    {
      "name": "Comma / Semicolon",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#8b847d"
      }
    },
    {
      "name": "TS Type annotation",
      "scope": [
        "punctuation.separator.type-annotation",
        "keyword.operator.type.annotation"
      ],
      "settings": {
        "foreground": "#8b847d"
      }
    },
    {
      "name": "TS type keyword",
      "scope": [
        "keyword.control.type",
        "storage.type.type",
        "storage.type.interface",
        "storage.type.enum"
      ],
      "settings": {
        "foreground": "#b6766c"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "meta.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#a27fa0"
      }
    },
    {
      "name": "TS namespace",
      "scope": [
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#acaa66"
      }
    },
    {
      "name": "Python self",
      "scope": [
        "variable.parameter.function.language.special.self.python"
      ],
      "settings": {
        "foreground": "#b6766c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python magic methods",
      "scope": [
        "support.function.magic.python"
      ],
      "settings": {
        "foreground": "#a27fa0"
      }
    },
    {
      "name": "Python decorator",
      "scope": [
        "entity.name.function.decorator.python",
        "meta.function.decorator.python"
      ],
      "settings": {
        "foreground": "#a27fa0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python f-string expression",
      "scope": [
        "meta.fstring variable",
        "meta.interpolation.python"
      ],
      "settings": {
        "foreground": "#c9bda8"
      }
    },
    {
      "name": "Python builtin type",
      "scope": [
        "support.type.python"
      ],
      "settings": {
        "foreground": "#acaa66"
      }
    },
    {
      "name": "Go package name",
      "scope": [
        "entity.name.package.go"
      ],
      "settings": {
        "foreground": "#acaa66"
      }
    },
    {
      "name": "Go import",
      "scope": [
        "entity.name.import.go"
      ],
      "settings": {
        "foreground": "#649064"
      }
    },
    {
      "name": "Go built-in function",
      "scope": [
        "support.function.builtin.go"
      ],
      "settings": {
        "foreground": "#d3b67c"
      }
    },
    {
      "name": "Go struct/type",
      "scope": [
        "variable.other.property.go",
        "entity.name.type.go"
      ],
      "settings": {
        "foreground": "#8098b1"
      }
    },
    {
      "name": "Go type keyword",
      "scope": [
        "keyword.struct.go",
        "keyword.interface.go",
        "keyword.map.go",
        "keyword.chan.go"
      ],
      "settings": {
        "foreground": "#b6766c"
      }
    },
    {
      "name": "JSON key",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#8098b1"
      }
    },
    {
      "name": "JSON string value",
      "scope": [
        "meta.structure.dictionary.value.json string.quoted"
      ],
      "settings": {
        "foreground": "#649064"
      }
    },
    {
      "name": "Markdown heading",
      "scope": [
        "heading.1.markdown entity.name",
        "heading.2.markdown entity.name",
        "heading.3.markdown entity.name",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#d3b67c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#acaa66",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#a27fa0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#8098b1"
      }
    },
    {
      "name": "Markdown code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#6db0aa"
      }
    },
    {
      "name": "HTML tag",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#b6766c"
      }
    },
    {
      "name": "HTML attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#d3b67c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX component",
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "foreground": "#acaa66"
      }
    },
    {
      "name": "CSS property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#8098b1"
      }
    },
    {
      "name": "CSS selector",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#d3b67c"
      }
    },
    {
      "name": "CSS value",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#c9bda8"
      }
    },
    {
      "name": "CSS unit",
      "scope": [
        "keyword.other.unit.css"
      ],
      "settings": {
        "foreground": "#b6766c"
      }
    },
    {
      "name": "Support variable / constant",
      "scope": [
        "support.variable",
        "support.constant"
      ],
      "settings": {
        "foreground": "#8098b1"
      }
    },
    {
      "name": "Entity name",
      "scope": [
        "entity.name"
      ],
      "settings": {
        "foreground": "#d3b67c"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#c4828f"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#c4828f",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function": "#d3b67c",
    "function.declaration": "#d3b67c",
    "method": "#d3b67c",
    "method.declaration": "#d3b67c",
    "variable": "#c9bda8",
    "variable.readonly": "#ba896d",
    "variable.declaration": "#c9bda8",
    "parameter": {
      "foreground": "#a9a292",
      "fontStyle": "italic"
    },
    "property": "#8098b1",
    "property.readonly": "#8098b1",
    "type": "#acaa66",
    "class": "#acaa66",
    "interface": {
      "foreground": "#acaa66",
      "fontStyle": "italic"
    },
    "enum": "#acaa66",
    "enumMember": "#8098b1",
    "namespace": "#acaa66",
    "typeParameter": "#6db0aa",
    "decorator": "#a27fa0",
    "macro": "#a27fa0"
  }
}