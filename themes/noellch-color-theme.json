    "list.hoverBackground": "#2f333b",
    "list.hoverForeground": "#cec5b5",
    "list.highlightForeground": "#c9b696",
    "list.focusBackground": "#3c4048",
    "list.focusForeground": "#d8ccb8",
    "statusBar.background": "#1e2129",
    "statusBar.foreground": "#8e8478",
    "statusBar.border": "#2a2d35",
    "statusBar.debuggingBackground": "#5c3a3a",
    "statusBar.debuggingForeground": "#d8ccb8",
    "statusBar.noFolderBackground": "#252831",
    "statusBarItem.activeBackground": "#42464e",
    "statusBarItem.hoverBackground": "#363a42",
    "statusBarItem.prominentBackground": "#363a42",
    "statusBarItem.remoteBackground": "#5e6269",
    "statusBarItem.remoteForeground": "#f0e8d8",
    "tab.activeBackground": "#252831",
    "tab.activeForeground": "#d8ccb8",
    "tab.inactiveBackground": "#1e2129",
    "tab.inactiveForeground": "#7a7876",
    "tab.border": "#1e2129",
    "tab.activeBorderTop": "#b1aa85",
    "tab.unfocusedActiveBorderTop": "#6b5a4a",
    "editorGroupHeader.tabsBackground": "#1e2129",
    "editorGroupHeader.tabsBorder": "#2a2d35",
    "terminal.background": "#252831",
    "terminal.foreground": "#cec5b5",
    "terminal.ansiBlack": "#363a42",
    "terminal.ansiRed": "#c298a0",
    "terminal.ansiGreen": "#91a087",
    "terminal.ansiYellow": "#b1aa85",
    "terminal.ansiBlue": "#8ea3ad",
    "terminal.ansiMagenta": "#a58c9f",
    "terminal.ansiCyan": "#82a599",
    "terminal.ansiWhite": "#cec5b5",
    "terminal.ansiBrightBlack": "#717782",
    "terminal.ansiBrightRed": "#c298a0",
    "terminal.ansiBrightGreen": "#97a48e",
    "terminal.ansiBrightYellow": "#b9b48c",
    "terminal.ansiBrightBlue": "#94a7b2",
    "terminal.ansiBrightMagenta": "#a992a3",
    "terminal.ansiBrightCyan": "#88aca2",
    "terminal.ansiBrightWhite": "#e0d6c4",
    "terminalCursor.foreground": "#c9b696",
    "scrollbar.shadow": "#161920",
    "scrollbarSlider.activeBackground": "#575b6266",
    "scrollbarSlider.background": "#45495044",
    "scrollbarSlider.hoverBackground": "#575b6255",
    "input.background": "#2a2d35",
    "input.foreground": "#cec5b5",
    "input.border": "#3c4048",
    "input.placeholderForeground": "#64686f",
    "inputOption.activeBorder": "#b1aa85",
    "inputOption.activeBackground": "#b1aa8533",
    "dropdown.background": "#2a2d35",
    "dropdown.border": "#3c4048",
    "dropdown.foreground": "#cec5b5",
    "button.background": "#5e6269",
    "button.foreground": "#f0e8d8",
    "button.hoverBackground": "#6f7379",
    "button.secondaryBackground": "#42464e",
    "button.secondaryForeground": "#cec5b5",
    "button.secondaryHoverBackground": "#52565e",
    "badge.background": "#b58a81",
    "badge.foreground": "#f0e8d8",
    "panel.background": "#20232b",
    "panel.border": "#363a42",
    "panelTitle.activeBorder": "#b1aa85",
    "panelTitle.activeForeground": "#d8ccb8",
    "panelTitle.inactiveForeground": "#7a7876",
    "peekView.border": "#5e6269",
    "peekViewEditor.background": "#252831",
    "peekViewEditor.matchHighlightBackground": "#b79c8a30",
    "peekViewResult.background": "#20232b",
    "peekViewResult.fileForeground": "#cec5b5",
    "peekViewResult.lineForeground": "#9a9590",
    "peekViewResult.matchHighlightBackground": "#b79c8a30",
    "peekViewResult.selectionBackground": "#3c4048",
    "peekViewResult.selectionForeground": "#d8ccb8",
    "peekViewTitle.background": "#2a2d35",
    "peekViewTitleDescription.foreground": "#8e8478",
    "peekViewTitleLabel.foreground": "#d8ccb8",
    "minimap.findMatchHighlight": "#b79c8a44",
    "minimap.selectionHighlight": "#52565e44",
    "minimapGutter.addedBackground": "#91a087",
    "minimapGutter.modifiedBackground": "#b1aa85",
    "minimapGutter.deletedBackground": "#c298a0",
    "breadcrumb.foreground": "#7a7876",
    "breadcrumb.focusForeground": "#cec5b5",
    "breadcrumb.activeSelectionForeground": "#d8ccb8",
    "breadcrumbPicker.background": "#2a2d35",
    "gitDecoration.addedResourceForeground": "#91a087",
    "gitDecoration.modifiedResourceForeground": "#b1aa85",
    "gitDecoration.deletedResourceForeground": "#c298a0",
    "gitDecoration.untrackedResourceForeground": "#82a599",
    "gitDecoration.ignoredResourceForeground": "#4e5259",
    "gitDecoration.conflictingResourceForeground": "#b79c8a",
    "notificationCenter.border": "#3c4048",
    "notificationCenterHeader.background": "#2a2d35",
    "notificationCenterHeader.foreground": "#cec5b5",
    "notifications.background": "#2a2d35",
    "notifications.border": "#3c4048",
    "notifications.foreground": "#cec5b5",
    "quickInput.background": "#2a2d35",
    "quickInput.foreground": "#cec5b5",
    "quickInputList.focusBackground": "#3c4048",
    "quickInputList.focusForeground": "#d8ccb8",
    "quickInputTitle.background": "#2a2d35",
    "diffEditor.insertedTextBackground": "#91a08718",
    "diffEditor.removedTextBackground": "#c298a018"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#717782",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#91a087"
      }
    },
    {
      "name": "String escape / Regex",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#82a599"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#b79c8a"
      }
    },
    {
      "name": "Boolean / Null / Undefined",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#b58a81"
      }
    },
    {
      "name": "Constant (other)",
      "scope": [
        "constant.other",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#b79c8a"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.new",
        "keyword.operator.delete",
        "keyword.operator.typeof",
        "keyword.operator.instanceof"
      ],
      "settings": {
        "foreground": "#b58a81"
      }
    },
    {
      "name": "Keyword operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#a59f96"
      }
    },
    {
      "name": "Storage (var, let, const, func, class)",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#b58a81"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "meta.function-call entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#c9b696"
      }
    },
    {
      "name": "Function call",
      "scope": [
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#c9b696"
      }
    },
    {
      "name": "Method",
      "scope": [
        "meta.method.declaration entity.name.function",
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#c9b696"
      }
    },
    {
      "name": "Class / Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#b1aa85"
      }
    },
    {
      "name": "Interface",
      "scope": [
        "entity.name.type.interface"
      ],
      "settings": {
        "foreground": "#b1aa85",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type parameter (generics)",
      "scope": [
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#82a599"
      }
    },
    {
      "name": "Enum member",
      "scope": [
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#8ea3ad"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#cec5b5"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter",
        "meta.parameters variable"
      ],
      "settings": {
        "foreground": "#b4ada0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#8ea3ad"
      }
    },
    {
      "name": "this / self",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.special.self"
      ],
      "settings": {
        "foreground": "#b58a81",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Import / Export / From",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#b58a81"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments"
      ],
      "settings": {
        "foreground": "#81858d"
      }
    },
    {
      "name": "Comma / Semicolon",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#81858d"
      }
    },
    {
      "name": "TS Type annotation",
      "scope": [
        "punctuation.separator.type-annotation",
        "keyword.operator.type.annotation"
      ],
      "settings": {
        "foreground": "#81858d"
      }
    },
    {
      "name": "TS type keyword",
      "scope": [
        "keyword.control.type",
        "storage.type.type",
        "storage.type.interface",
        "storage.type.enum"
      ],
      "settings": {
        "foreground": "#b58a81"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "meta.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#a58c9f"
      }
    },
    {
      "name": "TS namespace",
      "scope": [
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#b1aa85"
      }
    },
    {
      "name": "Python self",
      "scope": [
        "variable.parameter.function.language.special.self.python"
      ],
      "settings": {
        "foreground": "#b58a81",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python magic methods",
      "scope": [
        "support.function.magic.python"
      ],
      "settings": {
        "foreground": "#a58c9f"
      }
    },
    {
      "name": "Python decorator",
      "scope": [
        "entity.name.function.decorator.python",
        "meta.function.decorator.python"
      ],
      "settings": {
        "foreground": "#a58c9f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python f-string expression",
      "scope": [
        "meta.fstring variable",
        "meta.interpolation.python"
      ],
      "settings": {
        "foreground": "#cec5b5"
      }
    },
    {
      "name": "Python builtin type",
      "scope": [
        "support.type.python"
      ],
      "settings": {
        "foreground": "#b1aa85"
      }
    },
    {
      "name": "Go package name",
      "scope": [
        "entity.name.package.go"
      ],
      "settings": {
        "foreground": "#b1aa85"
      }
    },
    {
      "name": "Go import",
      "scope": [
        "entity.name.import.go"
      ],
      "settings": {
        "foreground": "#91a087"
      }
    },
    {
      "name": "Go built-in function",
      "scope": [
        "support.function.builtin.go"
      ],
      "settings": {
        "foreground": "#c9b696"
      }
    },
    {
      "name": "Go struct/type",
      "scope": [
        "variable.other.property.go",
        "entity.name.type.go"
      ],
      "settings": {
        "foreground": "#8ea3ad"
      }
    },
    {
      "name": "Go type keyword",
      "scope": [
        "keyword.struct.go",
        "keyword.interface.go",
        "keyword.map.go",
        "keyword.chan.go"
      ],
      "settings": {
        "foreground": "#b58a81"
      }
    },
    {
      "name": "JSON key",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#8ea3ad"
      }
    },
    {
      "name": "JSON string value",
      "scope": [
        "meta.structure.dictionary.value.json string.quoted"
      ],
      "settings": {
        "foreground": "#91a087"
      }
    },
    {
      "name": "Markdown heading",
      "scope": [
        "heading.1.markdown entity.name",
        "heading.2.markdown entity.name",
        "heading.3.markdown entity.name",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#c9b696",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#b1aa85",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#a58c9f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#8ea3ad"
      }
    },
    {
      "name": "Markdown code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#82a599"
      }
    },
    {
      "name": "HTML tag",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#b58a81"
      }
    },
    {
      "name": "HTML attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#c9b696",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX component",
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "foreground": "#b1aa85"
      }
    },
    {
      "name": "CSS property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#8ea3ad"
      }
    },
    {
      "name": "CSS selector",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#c9b696"
      }
    },
    {
      "name": "CSS value",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#cec5b5"
      }
    },
    {
      "name": "CSS unit",
      "scope": [
        "keyword.other.unit.css"
      ],
      "settings": {
        "foreground": "#b58a81"
      }
    },
    {
      "name": "Support variable / constant",
      "scope": [
        "support.variable",
        "support.constant"
      ],
      "settings": {
        "foreground": "#8ea3ad"
      }
    },
    {
      "name": "Entity name",
      "scope": [
        "entity.name"
      ],
      "settings": {
        "foreground": "#c9b696"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#c298a0"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#c298a0",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function": "#c9b696",
    "function.declaration": "#c9b696",
    "method": "#c9b696",
    "method.declaration": "#c9b696",
    "variable": "#cec5b5",
    "variable.readonly": "#b79c8a",
    "variable.declaration": "#cec5b5",
    "parameter": {
      "foreground": "#b4ada0",
      "fontStyle": "italic"
    },
    "property": "#8ea3ad",
    "property.readonly": "#8ea3ad",
    "type": "#b1aa85",
    "class": "#b1aa85",
    "interface": {
      "foreground": "#b1aa85",
      "fontStyle": "italic"
    },
    "enum": "#b1aa85",
    "enumMember": "#8ea3ad",
    "namespace": "#b1aa85",
    "typeParameter": "#82a599",
    "decorator": "#a58c9f",
    "macro": "#a58c9f"
  }
}