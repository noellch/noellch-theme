    "activityBar.inactiveForeground": "#585654",
    "activityBarBadge.background": "#b6766c",
    "activityBarBadge.foreground": "#f0e8d8",
    "sideBar.background": "#1a1918",
    "sideBar.foreground": "#9a9590",
    "sideBar.border": "#222120",
    "sideBarTitle.foreground": "#acaa66",
    "sideBarSectionHeader.background": "#1f1e1c",
    "sideBarSectionHeader.foreground": "#acaa66",
    "list.activeSelectionBackground": "#302f2e",
